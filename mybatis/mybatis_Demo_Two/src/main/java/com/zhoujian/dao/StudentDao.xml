<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zhoujian.dao.StudentDao">
    <select id="selectMethod" resultType="com.zhoujian.entity.Student">
        select * from student
    </select>
    <insert id="insertMethod">
        insert into student value( #{id},#{name},#{age},#{address})
    </insert>
    <update id="updateMethod">
        update student set name = #{name} where id = #{id}
    </update>
</mapper>
